<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shreypatel.dailogueguru">

    <uses-permission android:name="android.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".actor.akshaykumar" />
        <activity android:name=".actor.aamir_khan" />
        <activity android:name=".actor.shah_rukh" />
        <activity android:name=".actor.amitabh" />
        <activity android:name=".actor.abhishek" />
        <activity android:name=".actor.salman" />
        <activity android:name=".actor.irfan_khan" />
        <activity android:name=".actor.ajay_devgan" />
        <activity android:name=".actor.sanjay_dutt" />
        <activity android:name=".actor.anil_kapoor" />
        <activity android:name=".actor.sunny_deol" />
        <activity android:name=".actor.hrithik" />
        <activity android:name=".actor.john_abrahim" />
        <activity android:name=".actor.ranveer_singh" />

        <receiver android:name=".AlarmReceiver">
            <intent-filter>
                <action android:name="android.media.action.DISPLAY_NOTIFICATION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".more_apps" />
        <activity android:name=".About_us"></activity>
    </application>

</manifest>